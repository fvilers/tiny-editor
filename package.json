{
  "name": "react-tiny-editor",
  "version": "0.1.0",
  "description": "A tiny HTML rich text editor for React",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "start": "npx tsc && webpack-dev-server --open",
    "analyze": "npm run build && source-map-explorer dist/bundle.js",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jcable/react-tiny-editor.git"
  },
  "keywords": [
    "tiny",
    "editor",
    "wysiwyg",
    "html",
    "react",
    "javascript"
  ],
  "author": "jcable",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcable/react-tiny-editor/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-react": "^7.22.5",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-import-styles": "0.0.11",
    "babel-template": "^6.26.0",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "6.7.3",
    "eslint": "^8.48.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "5.5.0",
    "load-styles": "^2.0.0",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "source-map-explorer": "^1.6.0",
    "style-loader": "3.3.1",
    "typescript": "^5.2.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "4.1.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
